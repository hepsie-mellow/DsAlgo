<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DsAlgo">
	
<listeners>
	<listener class-name="com.dsalgo.utils.Listerners"/>
</listeners>

 <test thread-count="5"  name="chrome_test" >
   <parameter name="browser" value ="chrome"/>
     <classes>
     	<class name="dsAlgo_TestCases.Register_TestCase"/>
     	<class name="dsAlgo_TestCases.Login_Test"/>
     	<class name="dsAlgo_TestCases.DsAlgo_HomePage_TestCase" />
        <class name="dsAlgo_TestCases.DataStructure_TestCase" />
         <class name="dsAlgo_TestCases.Array_TestCases" />
         <class name="dsAlgo_TestCases.Graph_TestCases" />
          <class name="dsAlgo_TestCases.Stack_TestCases" />
           <class name="dsAlgo_TestCases.Queue_TestCases" />
            <class name="dsAlgo_TestCases.Tree_TestCases" />
             <class name="dsAlgo_TestCases.LinkedList_TestCase" />
    </classes>
  </test> 

 <test thread-count="5"  name="edge_test" >
   <parameter name="browser" value ="edge"/>
     <classes>
     	<class name="dsAlgo_TestCases.Register_TestCase"/>
     	<class name="dsAlgo_TestCases.Login_Test"/>
     	<class name="dsAlgo_TestCases.DsAlgo_HomePage_TestCase" />
        <class name="dsAlgo_TestCases.DataStructure_TestCase" />
         <class name="dsAlgo_TestCases.Array_TestCases" />
         <class name="dsAlgo_TestCases.Graph_TestCases" />
         <class name="dsAlgo_TestCases.Stack_TestCases" />
         <class name="dsAlgo_TestCases.Queue_TestCases" />
         <class name="dsAlgo_TestCases.Tree_TestCases" />
          <class name="dsAlgo_TestCases.LinkedList_TestCase" />
    </classes>
  </test> 
 
 </suite> 
